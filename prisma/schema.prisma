datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

model CheckIn {
  id String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  uid String
  location String
  timestamp DateTime @default(now())
}
